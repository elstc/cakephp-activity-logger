{
    "name": "elstc/cakephp-activity-logger",
    "description": "ActivityLogger plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": ["MIT"],
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.1",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Elastic\\ActivityLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elastic\\ActivityLogger\\Test\\": "tests",
            "Cake\\Test\\": "../../../vendor/cakephp/cakephp/tests"
        }
    }
}
