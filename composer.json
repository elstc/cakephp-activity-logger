{
    "name": "elastic/cakephp-activity-logger",
    "description": "ActivityLogger plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Elastic\\ActivityLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elastic\\ActivityLogger\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
